  * @brief   Default main function.
  ******************************************************************************
*/


#include "stm32f4xx.h"

#include "task.h"
			

int main(void)
{
	// 1,  RCC set to the default state ==> SCC - 16 MHz;
	RCC_DeInit();

	// 2. Update system SystemCoreClock variable
	SystemCoreClockUpdate();

	// 3. Create tasks;
	xTaskCreate();

	for(;;);



}
